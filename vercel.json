{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "^/assets/(.*)\\.js$",
      "headers": { "content-type": "application/javascript" },
      "dest": "/assets/$1.js"
    },
    {
      "src": "^/assets/(.*)\\.css$",
      "headers": { "content-type": "text/css" },
      "dest": "/assets/$1.css"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "github": {
    "silent": true
  }
}
